{
  "name": "@priosshrsth/assembly-kit",
  "version": "0.1.0",
  "description": "Alterntive sdk for Assembly platform",
  "homepage": "https://github.com/priosshrsth/assembly-kit#readme",
  "bugs": {
    "url": "https://github.com/priosshrsth/assembly-kit/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/priosshrsth/assembly-kit.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./app-bridge": {
      "import": {
        "types": "./dist/app-bridge/index.d.ts",
        "default": "./dist/app-bridge/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "format": "oxfmt",
    "lint": "oxlint",
    "prepare": "lefthook install",
    "release": "bumpp --commit --push --tag",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit",
    "check": "ultracite check",
    "fix": "ultracite fix"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "bumpp": "^10.4.1",
    "bunup": "^0.16.31",
    "lefthook": "^2.1.1",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "ultracite": "7.2.4"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "simple-git-hooks": {
    "pre-commit": "bun run lint && bun run type-check"
  }
}
